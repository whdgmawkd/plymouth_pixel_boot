# x += 240
# y -= 720

for (i = 480; i <= 720; i++)
  {
    image[i-480] = Image("ODR_Boot_02_" + i + ".png");
  }

index = 0;
frame = 0;
sprite = Sprite();

fun refresh_callback ()
  {
    frame++;
    if (frame <= 90)
      {
        sprite.SetImage(image[index]);
        sprite.SetX(offsets[index][0] + 240);
        sprite.SetY(offsets[index][1] - 720);
        return;
      }
    }
    index = frame - 90;
    sprite.SetX(offsets[index][0] + 240);
    sprite.SetY(offsets[index][1] - 720);
    sprite.SetImage(image[index]);
  }

Plymouth.SetRefreshFunction (refresh_callback);

